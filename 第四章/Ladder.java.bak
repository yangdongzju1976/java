public class Ladder 
{
	public static void main(String[] args) 
	{
		int i,j,k,m,t; //循环变量

		for (i=5;i<10 ;i++ ) //上半部分，i：控制上半部分的行数
		{
			for (j=1;j<10-i ;j++ ) //j：控制每一行的前导空格数
			{
				System.out.print(" ");
			}
			for (k=1;k<=2*i-1 ;k++ )
			{
				System.out.print("*");//k:控制每一行的*数
			}
			for (m=1;m<30-2*i ;m++ ) //j：控制每一行的前导空格数
			{
				System.out.print(" ");
			}
			for (t=1;t<=2*i-1 ;t++ )
			{
				System.out.print("*");//k:控制每一行的*数
			}
			for (m=1;m<30-2*i ;m++ ) //j：控制每一行的前导空格数
			{
				System.out.print(" ");
			}
			for (t=1;t<=2*i-1 ;t++ )
			{
				System.out.print("*");//k:控制每一行的*数
			}
			for (m=1;m<30-2*i ;m++ ) //j：控制每一行的前导空格数
			{
				System.out.print(" ");
			}
			for (t=1;t<=2*i-1 ;t++ )
			{
				System.out.print("*");//k:控制每一行的*数
			}
			System.out.println();
		}

		for (i=5;i<10 ;i++ )  //输出下半部分
		{
			for (j=1;j<i ;j++ )
			{
				System.out.print(" ");// 前导空格
			}
			for (k=1;k<=19-2*i ;k++ ) //输出*
			{
				System.out.print("*");
			}
			for (m=1;m<10+2*i ;m++ ) //输出第二个菱形的前导空格
			{
				System.out.print(" ");
			}
			for (t=1;t<=19-2*i ;t++ )
			{
				System.out.print("*");//输出第二个菱形的*
			}
			for (m=1;m<10+2*i ;m++ ) //输出第二个菱形的前导空格
			{
				System.out.print(" ");
			}
			for (t=1;t<=19-2*i ;t++ )
			{
				System.out.print("*");//输出第二个菱形的*
			}
			for (m=1;m<10+2*i ;m++ ) //输出第二个菱形的前导空格
			{
				System.out.print(" ");
			}
			for (t=1;t<=19-2*i ;t++ )
			{
				System.out.print("*");//输出第二个菱形的*
			}
			System.out.println();
		}

	}
}
